package fundamentos;

import java.util.Date; //Esta linha chama a bliblioteca semelhante ao python

public class Import {
	public static void main(String[] args) {
		//java.lang Ã© a lib padrao do java
		java.lang.String b = "Bom dia!";
		System.out.println(b);
		
		String c = "Boa tarde!";
		System.out.println(c);
		
		//Tipos que necessitam de lib externa precisam ser importados
		Date d = new Date();
		System.out.println(d);
	}
}
