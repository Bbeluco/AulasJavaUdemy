package fundamentos.operadores;

public class DesafioLogica {
	public static void main(String[] args) {
		boolean trabalho1 = true;
		boolean trabalho2 = true;
		
		System.out.println("Vai ao shopping? " + (trabalho1 || trabalho2));
		System.out.println("Comprar sorvete? " + (trabalho1 || trabalho2));
		System.out.println("Tem fome? " + !(trabalho1 || trabalho2));
	}
}
